#!/bin/bash
export SHARE=<%=@sharefolder%>/data
# Folder for workflow files
#DAG_DIR=/tmp/run
# Folder for files
#UPLOAD_DIR=/tmp/uploads
# Log directory
#LOGDIR=/var/log/{{ node_deb_package_name }}/
# HTCondor SOAP port
export CONDOR_URL=http://localhost:8080/
# HTCondor owner of job
export CONDOR_JOB_OWNER=rooot
# App portt
export PORT=<%= @port %>
# App listen adress
export HOST=<%= @listenaddr %>
# Database URI
export DATABASE_URI=<%= @mongoaddr %>
# Is development?
export NODE_ENV=production
# 0 : HTCondor
# 1 : Openlava
# 2 : Torque
# 3 : Slurm
export BMANAGER=0

