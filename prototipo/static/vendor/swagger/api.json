        },
        "/register": {
            "post": {
                "summary": "Create a new user",
                "description": "The register endpoint create a user.\nThe response includes a result of the operation\n",
                "consumes": [
                  "application/x-www-form-urlencoded"
                ],
                "parameters": [
                    {
                        "name": "username",
