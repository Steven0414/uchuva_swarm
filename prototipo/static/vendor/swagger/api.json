{
    "swagger": "2.0",
    "info": {
        "title": "Uchuva API",
        "description": "DAG manager",
        "version": "1.0.0"
    },
    "schemes": [
        "http"
    ],
    "basePath": "/",
    "consumes": [
    "application/json"
    ],
    "produces": [
      "application/json"
    ],
    "paths": {
        "/user": {
            "get": {
                "summary": "Get all dags",
                "description": "The user endpoint returns a list of dags\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Dag"
                ],
                "responses": {
                    "200": {
                        "description": "Array of user dags",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Dag"
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/crearDag": {
            "get": {
                "summary": "Create dag",
                "description": "The crearDag endpoint returns the name and id of a new created dag",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Dag"
                ],
                "responses": {
                    "200": {
                        "description": "Name and identifier of the dag",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "nombre": {
                                    "type": "string",
                                    "description": "Name of the dag"
                                },
                                "id": {
                                    "type": "string",
                                    "description": "Identifier of the dag"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/save": {
            "post": {
                "summary": "Save dag",
                "description": "The save endpoint edit a dag and returns the operation status",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                      "name": "body",
                      "in": "body",
                      "description": "All dag info",
                      "required": true,
                      "schema": {
                        "$ref": "#/definitions/DagSave"
                      }
                    }
                ],
                "tags": [
                    "Dag"
                ],
                "responses": {
                    "200": {
                        "description": "The result of the operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "number",
                                    "description": "Error number. None is 0"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/eliminarDag": {
            "get": {
                "summary": "Delete dag",
                "description": "The build endpoint returns information about the exectuions of a given\ngraphid. The response includes a list of execids\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Indentifier of the dag",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Dag"
                ],
                "responses": {
                    "200": {
                        "description": "The result of the operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "number",
                                    "description": "Error number. None is 0"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/run": {
            "post": {
                "summary": "Execute a Dag",
                "description": "The run endpoint start the execution of a given\ngraphid. The response includes a result code of the operation\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "body",
                        "in": "body",
                        "description": "All dag info",
                        "required": true,
                        "schema": {
                          "$ref": "#/definitions/Dag"
                        }
                    }
                ],
                "tags": [
                    "Run"
                ],
                "responses": {
                    "200": {
                        "description": "An array of price estimates by product",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "id of the dagExe"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/builds": {
            "get": {
                "summary": "Dag executions",
                "description": "The build endpoint returns information about the exectuions of a given\ngraphid. The response includes a list of execids\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Identifier of the dag.",
                        "required": true,
                        "type": "number",
                        "format": "string"
                    }
                ],
                "tags": [
                    "Build"
                ],
                "responses": {
                    "200": {
                        "description": "An array of dags executions",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/DagExe"
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/build": {
            "get": {
                "summary": "Execution summary",
                "description": "The build endpoint returns information about the exectuion of a given\nexecid. The response includes a DagExe\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Identifier of the execution.",
                        "required": true,
                        "type": "number",
                        "format": "string"
                    }
                ],
                "tags": [
                    "Build"
                ],
                "responses": {
                    "200": {
                        "description": "Execution dag",
                        "schema": {
                            "$ref": "#/definitions/DagExe"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/dataNodeDag": {
            "post": {
                "summary": "Information about a node",
                "description": "The dataNodeDag endpoint returns information about the exectuions of a given\nnode of a graphExeid. The response varies from error, log and output information\nparameters:\n- $ref: '#/definitions/Node'\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                      "name": "body",
                      "in": "body",
                      "description": "All the node info",
                      "required": true,
                      "schema": {
                        "$ref": "#/definitions/Datanode"
                      }
                    }
                ],
                "tags": [
                    "Build"
                ],
                "responses": {
                    "200": {
                        "description": "The information you just be waiting for",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "tipo": {
                                    "type": "string",
                                    "description": "Type of info, err, log or out"
                                },
                                "info": {
                                    "type": "string",
                                    "description": "the info"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/listar": {
            "post": {
                "summary": "List files and folders",
                "description": "The listar endpoint returns a list of files and folders from an user\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "body",
                        "in": "body",
                        "description": "path of the folder",
                        "required": false,
                        "schema": {
                            "$ref": "#/definitions/pathcwd"
                        }
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Files of the user",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "array",
                                    "description": "array of files",
                                    "items": {
                                      "$ref": "#/definitions/Fileview"
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/crearArchivo": {
            "post": {
                "summary": "Create file",
                "description": "The crearArchivos endpoint save a file in the user fs.\nThe response includes a result of the operation\n",
                "consumes": [
                  "multipart/form-data"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "cwd",
                        "in": "formData",
                        "description": "Identifier of the dag.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "file",
                        "in": "formData",
                        "description": "File",
                        "required": true,
                        "type": "file"
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Result of the operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "string",
                                    "description": "Error number. None is 0"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/descargarArchivo": {
            "get": {
                "summary": "Download a file",
                "description": "Download a sigle file",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "path",
                        "in": "query",
                        "description": "Indentifier of the file",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Name and identifier of the dag",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/eliminarArchivo": {
            "post": {
                "summary": "Delete files",
                "description": "The eliminarArchivo endpoint deletes a set of files from vfs.\nThe response includes the result code of the operation\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                      "name": "item",
                      "in": "body",
                      "description": "Id of the file",
                      "required": true,
                      "schema": {
                        "type": "string"
                      }
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Result of the operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "number",
                                    "description": "Id of the file"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/contenidoArchivo": {
            "post": {
                "summary": "Get the content of the file",
                "description": "The contenidoArchivo endpoint getst the content of a file from vfs.\nThe response includes the content of the file\n",
                "consumes": [
                  "application/json"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                      "name": "item",
                      "in": "body",
                      "description": "Id of the file",
                      "required": true,
                      "schema": {
                        "type": "string"
                      }
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Content of the file",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "string",
                                    "description": "Content of the file"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/crearCarpeta": {
            "post": {
                "summary": "Create a folder in the VFS",
                "description": "The crearCarpeta endpoint create a folder in the vfs.\nThe response includes a result code of the operation path\n",
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "newPath",
                        "in": "body",
                        "description": "item to rename",
                        "schema": {
                          "$ref": "#/definitions/Folder"
                        }
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "The result of the operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "number",
                                    "description": "result of create folder"
                                },
                                "id": {
                                    "type": "string",
                                    "description": "result of create folder"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/buscar": {
            "post": {
                "summary": "Search a file by filename",
                "description": "The register endpoint search a file.\nThe response includes an array of coincidences\n",
                "consumes": [
                  "application/x-www-form-urlencoded"
                ],
                "parameters": [
                    {
                        "name": "apikey",
                        "in": "header",
                        "description": "token to be passed as a header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "filename",
                        "in": "formData",
                        "description": "File-name.",
                        "required": false,
                        "type": "string"
                    }
                ],
                "tags": [
                    "VFS"
                ],
                "responses": {
                    "200": {
                        "description": "Files of the user",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "array",
                                    "description": "array of files",
                                    "items": {
                                      "$ref": "#/definitions/Fileview"
